{
    "name": "luilliarcec/laravel-components",
    "description": "Blade components (Tailwind, AlpineJS, Blade).",
    "keywords": [
        "components",
        "laravel",
        "blade",
        "tailwind"
    ],
    "homepage": "https://github.com/luilliarcec/laravel-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Andr√©s Arce C.",
            "email": "luilliarcec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "blade-ui-kit/blade-icons": "^1.3",
        "laravel/framework": "^9.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0",
        "luilliarcec/whetstone": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Luilliarcec\\Components\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luilliarcec\\Components\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luilliarcec\\Components\\ComponentsServiceProvider"
            ]
        }
    }
}
